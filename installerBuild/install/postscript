| readme |
readme := 'ReadMe.txt'.
(#('3.8' '3.9' '3.10' '4.1') detect: [:each | '*',each,'*' match: Smalltalk version] ifNone: []) ifNotNil: [readme := 'ReadMeOLD.txt'].

StringHolder new
		acceptContents: ((self memberNamed: readme ) contents convertFromWithConverter: (UTF8TextConverter new));
		openLabel: 'Read Me'.
