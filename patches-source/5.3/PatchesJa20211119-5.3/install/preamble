membersInstaller := [:membersArg |
	membersArg do: [:each | 
		[self fileInMemberNamed: each fileName] on: Error do: [:ex | Transcript cr; show: (each fileName, ' not loaded.')]
	]
].

members := (self membersMatching: '5.3-extra-patches/*').
members := members reject: [:each | each fileName endsWith: '/'].
membersInstaller value: members.

(Smalltalk at: #PatchesJa ifAbsentPut: [OrderedCollection new]) asArray.
(PatchesJa includes: self fileName) ifFalse: [PatchesJa add: self fileName].

